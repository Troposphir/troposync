{
  "name": "troposync",
  "productName": "Troposphir Launcher",
  "version": "1.0.0",
  "description": "Launcher and updater for Troposphir",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "tslint src",
    "serve": "./node_modules/.bin/ts-node src/server/index.ts"
  },
  "keywords": [
    "launcher",
    "updater",
    "game"
  ],
  "author": "The Troposphir Team",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "ignore": [
          "/\\.idea($|/)",
          "/\\.sync($|/)",
          "/node_modules/ts-node($|/)",
          "\\.d\\.ts$"
        ]
      },
      "electronWinstallerConfig": {
        "name": "troposync"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Troposphir"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "troposync"
      }
    }
  },
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "@types/electron": "^1.4.32",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.53",
    "@types/morgan": "^1.7.32",
    "electron-compile": "^6.0.0",
    "electron-devtools-installer": "^2.1.0",
    "electron-log": "^1.3.0",
    "express": "^4.14.1",
    "fs-promise": "^2.0.0",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "promise-mysql": "^3.0.0",
    "promise-timeout": "^1.0.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.1.1",
    "ts-node": "^2.1.0",
    "tslib": "^1.5.0",
    "typescript": "2.2.0",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "electron-prebuilt-compile": "1.4.15",
    "tslint": "^4.4.2",
    "typescript": "2.2.0"
  }
}
